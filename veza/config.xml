<?xml version='1.0' encoding='utf-8'?>
<widget id="com.woxi.veza" version="1.8" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Veza</name>
